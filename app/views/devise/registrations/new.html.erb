


<head>
  <link rel="stylesheet" href="/Hackathon_Blog/app/assets/stylesheets/login.scss">
  <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.1/css/all.css" integrity="sha384-vp86vTRFVJgpjF9jiIGPEEqYqlDwgyBgEF109VFjmqGmIY/Y4HV4d3Gp2irVfcrp" crossorigin="anonymous">
  </head>
  <!------ Include the above in your HEAD tag ---------->
  <body>
    <%= render 'shared/navbar' %>
  <div class="wrapper fadeInDown">
    <div id="formContent">
      <!-- Tabs Titles -->
  
      <!-- Icon -->
      <div class="fadeIn first">
         <i class="fas fa-user-circle fa-5x" id="icon" alt="User Icon"></i> 
      </div>
  
      <!-- Signup Form -->
     
     
        <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <div class="form-inputs">
         <%= f.error_notification %>

     
    <%= f.input :fullname, 
    :class => '', 
    :placeholder => 'Full Name',
    label: false,
     autofocus: true,
     input_html: { class: 'form_text' } %>
     
    <%= f.input :email,
                required: true,
                label: false,
                placeholder: 'Email',
                autofocus: true,
                input_html: { autocomplete: "email", class: 'form_text' }%>
     
    <%= f.input :password,
                required: true,
                label: false,
                placeholder: 'Password',
                hint: ("please use a minimum of #{@minimum_password_length} characters" if @minimum_password_length),
                input_html: { autocomplete: "new-password", class: 'form_text' } %>
      
    <%= f.input :password_confirmation,
                required: true,
                label: false,
                placeholder: 'Password',
                input_html: { autocomplete: "new-password", class: 'form_text' } %>
      
        <div class="form-actions">
          <%= f.button :submit, "Sign Up" %>
        </div>
      <% end %>
  
    </div>
  </div>
  <%= render "devise/shared/links" %>
  </body>